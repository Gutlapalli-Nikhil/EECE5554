<launch>
	
	<arg name="imu_port" />
	<param name="imu_port" value="$(arg imu_port)" />

	<arg name="gps_port"/>
	<param name="gps_port" value="$(arg gps_port)" />

	<node pkg="sensor_fusion" type="driver.py" name="driver" output="screen">
		
	</node>
	
	<node pkg="sensor_fusion" type="server.py" name="eigan_to_quaternion_server" output="screen">
	</node>

	<node pkg="sensor_fusion" type="gps_driver.py" name="gps_data" output="screen">
		
	</node>

</launch>
